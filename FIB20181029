//Fibonacci Series using Recursion 
#include<iostream>
#include<stdio.h>
#include<cstdlib>
#include<math.h>


using namespace std;


int fib(int n) 
{ 
if (n <= 1) 
	return n; 
return fib(n-1) + fib(n-2); 
} 




int main ()
{
    int n;
    int num;
    int radius;
    

    srand(time(NULL));
    num = rand()%18+2;
    cout <<num<<endl;
    n = fib(num);
    cout << n<<endl;
    radius = n;
    cout << radius;
}
